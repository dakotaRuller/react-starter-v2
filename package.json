    "jest-svg-transformer": "^1.0.0",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
